package oi.github.josepaulo.data.models;

import java.util.Date;

import jakarta.annotation.Generated;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import oi.github.josepaulo.domain.entities.Product;

public class ProductModel {
	
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private Long id;
	private String nome;
	private String descricao;
	private Date dataCriacao;
	private int qtdMinima;
	private int qtdMaxima;
	
	public ProductModel(Long id, String nome, String descricao, Date dataCriacao, int qtdMinima, int qtdMaxima) {
		this.id = id;
		this.nome = nome;
		this.descricao = descricao;
		this.dataCriacao = dataCriacao;
		this.qtdMinima = qtdMinima;
		this.qtdMaxima = qtdMaxima;
	}
	
	
	public ProductModel toProductModel(Product product) {
		ProductModel productModel =  new ProductModel();
	}
	
	
	
	
}
