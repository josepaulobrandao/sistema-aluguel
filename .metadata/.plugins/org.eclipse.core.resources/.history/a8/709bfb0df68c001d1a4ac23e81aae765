package oi.github.josepaulo.data.models;

import java.util.Date;
import java.util.Objects;

import jakarta.annotation.Generated;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import oi.github.josepaulo.domain.entities.Product;

public class ProductModel {
	
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private Long id;
	private String nome;
	private String descricao;
	private Date dataCriacao;
	private int qtdMinima;
	private int qtdMaxima;
	
	public ProductModel(Long id, String nome, String descricao, Date dataCriacao, int qtdMinima, int qtdMaxima) {
		this.id = id;
		this.nome = nome;
		this.descricao = descricao;
		this.dataCriacao = dataCriacao;
		this.qtdMinima = qtdMinima;
		this.qtdMaxima = qtdMaxima;
	}
	
	
	public Product toProductModel(Product product) {
		ProductModel productModel =  new ProductModel();

	}


	public Long getId() {
		return id;
	}


	public void setId(Long id) {
		this.id = id;
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}


	public String getDescricao() {
		return descricao;
	}


	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}


	public Date getDataCriacao() {
		return dataCriacao;
	}


	public void setDataCriacao(Date dataCriacao) {
		this.dataCriacao = dataCriacao;
	}


	public int getQtdMinima() {
		return qtdMinima;
	}


	public void setQtdMinima(int qtdMinima) {
		this.qtdMinima = qtdMinima;
	}


	public int getQtdMaxima() {
		return qtdMaxima;
	}


	public void setQtdMaxima(int qtdMaxima) {
		this.qtdMaxima = qtdMaxima;
	}


	@Override
	public int hashCode() {
		return Objects.hash(dataCriacao, descricao, id, nome, qtdMaxima, qtdMinima);
	}


	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		ProductModel other = (ProductModel) obj;
		return Objects.equals(dataCriacao, other.dataCriacao) && Objects.equals(descricao, other.descricao)
				&& Objects.equals(id, other.id) && Objects.equals(nome, other.nome) && qtdMaxima == other.qtdMaxima
				&& qtdMinima == other.qtdMinima;
	}
	
	
	
	
	
}
